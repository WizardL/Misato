{
  "parser": "babel-eslint",
  "extends": "standard",
  "plugins": [
    "babel"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-extra-parens": 0,
    "space-before-function-paren": [
      2,
      "never"
    ],
    "no-duplicate-imports": 0,
    "no-useless-escape": 0,
    "no-unneeded-ternary": 0,
    "curly": 0,
    "no-var": 2,
    "no-return-assign": 0,
    "camelcase": 0,
    "semi": [
      2,
      "never"
    ],
    "eqeqeq": 0,
    "arrow-parens": [
      2,
      "always"
    ],
    "arrow-spacing": 2,
    "yoda": 0,
    "dot-location": [
      2,
      "property"
    ],
    "prefer-arrow-callback": 2
  }
}